{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "premierleague-datafactory"
		},
		"AzureSqlDatabase1_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'AzureSqlDatabase1'"
		},
		"Batch_storage_linked_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'Batch_storage_linked'"
		},
		"Premierleagecsvfilea_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'Premierleagecsvfilea'"
		},
		"batch_acc_linked_accessKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accessKey' of 'batch_acc_linked'"
		},
		"newcsvfiles_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'newcsvfiles'"
		},
		"output_data_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'output_data'"
		},
		"outputcsv_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'outputcsv'"
		},
		"AzureSqlDatabase1_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "sshresthapremierleage.database.windows.net"
		},
		"AzureSqlDatabase1_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "sqldb"
		},
		"AzureSqlDatabase1_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "sshrestha"
		},
		"Premierleagecsvfilea_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://sshrestha.dfs.core.windows.net/"
		},
		"batch_acc_linked_properties_typeProperties_batchUri": {
			"type": "string",
			"defaultValue": "https://shresthaplbatch.canadacentral.batch.azure.com"
		},
		"batch_acc_linked_properties_typeProperties_poolName": {
			"type": "string",
			"defaultValue": "shrestha_pl_pool"
		},
		"batch_acc_linked_properties_typeProperties_accountName": {
			"type": "string",
			"defaultValue": "shresthaplbatch"
		},
		"newcsvfiles_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://shrestha.dfs.core.windows.net/"
		},
		"output_data_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://sshrestha.dfs.core.windows.net/"
		},
		"outputcsv_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://sshrestha.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "premierleague-datafactory_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureSqlDatabase1_password": {
						"value": "[parameters('AzureSqlDatabase1_password')]"
					},
					"Batch_storage_linked_connectionString": {
						"value": "[parameters('Batch_storage_linked_connectionString')]"
					},
					"Premierleagecsvfilea_accountKey": {
						"value": "[parameters('Premierleagecsvfilea_accountKey')]"
					},
					"newcsvfiles_accountKey": {
						"value": "[parameters('newcsvfiles_accountKey')]"
					},
					"output_data_accountKey": {
						"value": "[parameters('output_data_accountKey')]"
					},
					"outputcsv_accountKey": {
						"value": "[parameters('outputcsv_accountKey')]"
					},
					"batch_acc_linked_accessKey": {
						"value": "[parameters('batch_acc_linked_accessKey')]"
					},
					"AzureSqlDatabase1_properties_typeProperties_server": {
						"value": "[parameters('AzureSqlDatabase1_properties_typeProperties_server')]"
					},
					"AzureSqlDatabase1_properties_typeProperties_database": {
						"value": "[parameters('AzureSqlDatabase1_properties_typeProperties_database')]"
					},
					"AzureSqlDatabase1_properties_typeProperties_userName": {
						"value": "[parameters('AzureSqlDatabase1_properties_typeProperties_userName')]"
					},
					"Premierleagecsvfilea_properties_typeProperties_url": {
						"value": "[parameters('Premierleagecsvfilea_properties_typeProperties_url')]"
					},
					"newcsvfiles_properties_typeProperties_url": {
						"value": "[parameters('newcsvfiles_properties_typeProperties_url')]"
					},
					"output_data_properties_typeProperties_url": {
						"value": "[parameters('output_data_properties_typeProperties_url')]"
					},
					"outputcsv_properties_typeProperties_url": {
						"value": "[parameters('outputcsv_properties_typeProperties_url')]"
					},
					"batch_acc_linked_properties_typeProperties_batchUri": {
						"value": "[parameters('batch_acc_linked_properties_typeProperties_batchUri')]"
					},
					"batch_acc_linked_properties_typeProperties_poolName": {
						"value": "[parameters('batch_acc_linked_properties_typeProperties_poolName')]"
					},
					"batch_acc_linked_properties_typeProperties_accountName": {
						"value": "[parameters('batch_acc_linked_properties_typeProperties_accountName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}